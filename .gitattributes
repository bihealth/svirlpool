# SCM syntax highlighting & preventing 3-way merges
pixi.lock merge=binary linguist-language=YAML linguist-generated=true
data/**/* filter=lfs diff=lfs merge=lfs -text
examples/*.bam filter=lfs diff=lfs merge=lfs -text
examples/*.bam.bai filter=lfs diff=lfs merge=lfs -text
examples/*.fa filter=lfs diff=lfs merge=lfs -text
examples/*.bed filter=lfs diff=lfs merge=lfs -text
